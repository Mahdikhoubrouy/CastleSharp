<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <PackageIcon></PackageIcon>
    <RepositoryUrl>https://github.com/Mahdikhoubrouy/CastleSharp</RepositoryUrl>
    <PackageTags>TelegramBot,Telegram,Bot,TelegramTools,Tools,CastleSharp</PackageTags>
    <FileVersion>2.0.0</FileVersion>
    <AssemblyVersion>2.0.0</AssemblyVersion>
	<Version>2.0.0</Version>
    <PackageReleaseNotes></PackageReleaseNotes>
    <Title>CastleSharp</Title>
    <PackageId>CastleSharp</PackageId>
    <Authors>CastleSharp</Authors>
    <Product>CastleSharp</Product>
    <Description>a tool for telegram.bot </Description>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Telegram.Bot" Version="[19.*,)" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
